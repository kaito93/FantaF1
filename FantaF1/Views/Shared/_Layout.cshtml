<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/css/bootstrap-select.min.css">

</head>
<body>

    <div class="container body-content">

        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <a class="navbar-brand" href="#">FantaF1 Tool</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto" style="text-align: center; border-bottom-style:solid; border-bottom-color:rgba(0,0,0,0.15); border-bottom-width:2px;">
                    <li class="nav-item active">
                        <a id="btniscriviutenti" class="nav-link" href="javascript: LoadIscrizioniUtentiSection();">Carica Iscrizioni Utenti</a>
                    </li>
                    <li class="nav-item">
                        <a id="btnInserisciPronostici" class="nav-link" href="javascript: LoadPronosticiUtentiSection();">Carica Pronostici Utenti</a>
                    </li>
                    <li class="nav-item">
                        <a id="btnSetResultRace" class="nav-link" href="javascript: LoadRisultatiGaraSection();">Inserisci Risultati Gara</a>
                    </li>
                    <li class="nav-item">
                        <a id="btnCalcolaRisultatiPronostici" class="nav-link" href="javascript: LoadCalcolaPronosticiGara();">Calcola Pronostici Utenti</a>
                    </li>
                    <li class="nav-item">
                        <a id="ScaricaRisultatiPronostici" class="nav-link" href="javascript: LoadScaricaRisultatiPronosticiGara();">Scarica risultati pronostici utenti</a>
                    </li>
                </ul>
            </div>
        </nav>


        @RenderBody()
    </div>

    <!-- JS, Popper.js, and jQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap-select@1.13.14/dist/js/bootstrap-select.min.js"></script>

    @RenderSection("scripts", required: false)
    <script type="text/javascript" src="~/Scripts/Js/Index.js"></script>
</body>
</html>
